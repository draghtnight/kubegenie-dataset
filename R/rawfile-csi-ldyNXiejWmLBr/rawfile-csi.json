  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-rawfile-csi-controller",
      "labels": {
        "helm.sh/chart": "rawfile-csi-0.2.1",
        "app.kubernetes.io/name": "rawfile-csi",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "0.2.1",
        "app.kubernetes.io/managed-by": "Helm",
        "component": "controller"
      }
    },
    "spec": {
      "type": "ClusterIP",
      "selector": {
        "app.kubernetes.io/name": "rawfile-csi",
        "app.kubernetes.io/instance": "release-name",
        "component": "controller"
      },
      "clusterIP": "None"
    }
  },
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-rawfile-csi-node",
      "labels": {
        "helm.sh/chart": "rawfile-csi-0.2.1",
        "app.kubernetes.io/name": "rawfile-csi",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "0.2.1",
        "app.kubernetes.io/managed-by": "Helm",
        "component": "node"
      }
    },
    "spec": {
      "type": "ClusterIP",
      "ports": [
        {
          "name": "metrics",
          "port": 9100,
          "targetPort": "metrics",
          "protocol": "TCP"
        }
      ],
      "selector": {
        "app.kubernetes.io/name": "rawfile-csi",
        "app.kubernetes.io/instance": "release-name",
        "component": "node"
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "DaemonSet",
    "metadata": {
      "name": "release-name-rawfile-csi-node"
    },
    "spec": {
      "updateStrategy": {
        "rollingUpdate": {
          "maxUnavailable": "100%"
        }
      },
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "rawfile-csi",
          "app.kubernetes.io/instance": "release-name",
          "component": "node"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app.kubernetes.io/name": "rawfile-csi",
            "app.kubernetes.io/instance": "release-name",
            "component": "node"
          }
        },
        "spec": {
          "serviceAccount": "release-name-rawfile-csi-driver",
          "priorityClassName": "system-node-critical",
          "tolerations": [
            {
              "operator": "Exists"
            }
          ],
          "volumes": [
            {
              "name": "csi-metadata-dir",
              "hostPath": {
                "path": "/etc/rawfile-csi",
                "type": "DirectoryOrCreate"
              }
            },
            {
              "name": "registration-dir",
              "hostPath": {
                "path": "/var/lib/kubelet/plugins_registry",
                "type": "Directory"
              }
            },
            {
              "name": "socket-dir",
              "hostPath": {
                "path": "/var/lib/kubelet/plugins/rawfile-csi",
                "type": "DirectoryOrCreate"
              }
            },
            {
              "name": "mountpoint-dir",
              "hostPath": {
                "path": "/var/lib/kubelet",
                "type": "DirectoryOrCreate"
              }
            },
            {
              "name": "data-dir",
              "hostPath": {
                "path": "/",
                "type": "DirectoryOrCreate"
              }
            }
          ],
          "containers": [
            {
              "name": "csi-driver",
              "image": "matrixdb/rawfile-csi:v0.2.1",
              "imagePullPolicy": "IfNotPresent",
              "securityContext": {
                "privileged": true,
                "allowPrivilegeEscalation": false,
                "capabilities": {
                  "drop": {
                    "": "NET_RAW"
                  }
                },
                "readOnlyRootFilesystem": true
              },
              "env": [
                {
                  "name": "PROVISIONER_NAME",
                  "value": "rawfile.csi.ymatrix.cn"
                },
                {
                  "name": "CSI_ENDPOINT",
                  "value": "unix:///csi/csi.sock"
                },
                {
                  "name": "IMAGE_REPOSITORY",
                  "value": "matrixdb/rawfile-csi"
                },
                {
                  "name": "NODE_ID",
                  "valueFrom": {
                    "fieldRef": {
                      "apiVersion": "v1",
                      "fieldPath": "spec.nodeName"
                    }
                  }
                }
              ],
              "ports": [
                {
                  "name": "metrics",
                  "containerPort": 9100
                },
                {
                  "name": "csi-probe",
                  "containerPort": 9808
                }
              ],
              "livenessProbe": {
                "failureThreshold": 5,
                "httpGet": {
                  "path": "/healthz",
                  "port": "csi-probe"
                },
                "initialDelaySeconds": 10,
                "timeoutSeconds": 5,
                "periodSeconds": 5
              },
              "volumeMounts": [
                {
                  "name": "csi-metadata-dir",
                  "mountPath": "/etc/csi"
                },
                {
                  "name": "socket-dir",
                  "mountPath": "/csi"
                },
                {
                  "name": "mountpoint-dir",
                  "mountPath": "/var/lib/kubelet",
                  "mountPropagation": "Bidirectional"
                },
                {
                  "name": "data-dir",
                  "mountPath": "/data"
                }
              ],
              "resources": {
                "limits": {
                  "cpu": 1,
                  "memory": "100Mi"
                },
                "requests": {
                  "cpu": "10m",
                  "memory": "100Mi"
                }
              }
            },
            {
              "name": "node-driver-registrar",
              "image": "matrixdb/sig-storage_csi-node-driver-registrar:v2.2.0",
              "imagePullPolicy": "IfNotPresent",
              "args": [
                "--csi-address=$(ADDRESS)",
                "--kubelet-registration-path=$(DRIVER_REG_SOCK_PATH)",
                "--health-port=9809"
              ],
              "env": [
                {
                  "name": "ADDRESS",
                  "value": "/csi/csi.sock"
                },
                {
                  "name": "DRIVER_REG_SOCK_PATH",
                  "value": "/var/lib/kubelet/plugins/rawfile-csi/csi.sock"
                }
              ],
              "ports": [
                {
                  "containerPort": 9809,
                  "name": "healthz"
                }
              ],
              "livenessProbe": {
                "httpGet": {
                  "path": "/healthz",
                  "port": "healthz"
                },
                "initialDelaySeconds": 5,
                "timeoutSeconds": 5
              },
              "volumeMounts": [
                {
                  "name": "socket-dir",
                  "mountPath": "/csi"
                },
                {
                  "name": "registration-dir",
                  "mountPath": "/registration"
                }
              ],
              "resources": {
                "limits": {
                  "cpu": "500m",
                  "memory": "100Mi"
                },
                "requests": {
                  "cpu": "10m",
                  "memory": "100Mi"
                }
              }
            },
            {
              "name": "external-provisioner",
              "image": "matrixdb/custom-external-provisioner:4622a07d7-20220424",
              "imagePullPolicy": "IfNotPresent",
              "args": [
                "--csi-address=$(ADDRESS)",
                "--feature-gates=Topology=true",
                "--strict-topology",
                "--immediate-topology=false",
                "--timeout=120s",
                "--enable-capacity=true",
                "--capacity-ownerref-level=1",
                "--node-deployment=true"
              ],
              "env": [
                {
                  "name": "ADDRESS",
                  "value": "/csi/csi.sock"
                },
                {
                  "name": "NODE_NAME",
                  "valueFrom": {
                    "fieldRef": {
                      "fieldPath": "spec.nodeName"
                    }
                  }
                },
                {
                  "name": "NAMESPACE",
                  "valueFrom": {
                    "fieldRef": {
                      "fieldPath": "metadata.namespace"
                    }
                  }
                },
                {
                  "name": "POD_NAME",
                  "valueFrom": {
                    "fieldRef": {
                      "fieldPath": "metadata.name"
                    }
                  }
                }
              ],
              "volumeMounts": [
                {
                  "name": "socket-dir",
                  "mountPath": "/csi"
                }
              ]
            },
            {
              "name": "liveness-probe",
              "imagePullPolicy": "IfNotPresent",
              "image": "matrixdb/sig-storage_livenessprobe:v2.3.0",
              "args": [
                "--csi-address=/csi/csi.sock"
              ],
              "volumeMounts": [
                {
                  "name": "socket-dir",
                  "mountPath": "/csi"
                }
              ]
            }
          ]
        }
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "StatefulSet",
    "metadata": {
      "name": "release-name-rawfile-csi-controller"
    },
    "spec": {
      "replicas": 1,
      "serviceName": "release-name-rawfile-csi",
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "rawfile-csi",
          "app.kubernetes.io/instance": "release-name",
          "component": "controller"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app.kubernetes.io/name": "rawfile-csi",
            "app.kubernetes.io/instance": "release-name",
            "component": "controller"
          }
        },
        "spec": {
          "serviceAccount": "release-name-rawfile-csi-driver",
          "priorityClassName": "system-cluster-critical",
          "tolerations": [
            {
              "key": "node-role.kubernetes.io/master",
              "operator": "Equal",
              "value": "true",
              "effect": "NoSchedule"
            }
          ],
          "volumes": [
            {
              "name": "socket-dir",
              "emptyDir": {}
            }
          ],
          "containers": [
            {
              "name": "csi-driver",
              "image": "matrixdb/rawfile-csi:v0.2.1",
              "imagePullPolicy": "IfNotPresent",
              "args": [
                "csi-driver",
                "--disable-metrics"
              ],
              "env": [
                {
                  "name": "NODE_ID",
                  "value": "ControllerService"
                },
                {
                  "name": "PROVISIONER_NAME",
                  "value": "rawfile.csi.ymatrix.cn"
                },
                {
                  "name": "CSI_ENDPOINT",
                  "value": "unix:///csi/csi.sock"
                },
                {
                  "name": "IMAGE_REPOSITORY",
                  "value": "matrixdb/rawfile-csi"
                }
              ],
              "volumeMounts": [
                {
                  "name": "socket-dir",
                  "mountPath": "/csi"
                }
              ],
              "ports": [
                {
                  "name": "csi-probe",
                  "containerPort": 9808,
                  "protocol": "TCP"
                }
              ],
              "livenessProbe": {
                "failureThreshold": 5,
                "httpGet": {
                  "path": "/healthz",
                  "port": "csi-probe"
                },
                "initialDelaySeconds": 10,
                "timeoutSeconds": 5,
                "periodSeconds": 5
              },
              "resources": {
                "limits": {
                  "cpu": 1,
                  "memory": "100Mi"
                },
                "requests": {
                  "cpu": "10m",
                  "memory": "100Mi"
                }
              }
            },
            {
              "name": "liveness-probe",
              "imagePullPolicy": "IfNotPresent",
              "image": "matrixdb/sig-storage_livenessprobe:v2.3.0",
              "args": [
                "--csi-address=/csi/csi.sock"
              ],
              "volumeMounts": [
                {
                  "name": "socket-dir",
                  "mountPath": "/csi"
                }
              ]
            }
          ]
        }
      }
    }
  },
  {
    "apiVersion": "storage.k8s.io/v1",
    "kind": "CSIDriver",
    "metadata": {
      "name": "rawfile.csi.ymatrix.cn"
    },
    "spec": {
      "attachRequired": false,
      "podInfoOnMount": true,
      "fsGroupPolicy": "File",
      "storageCapacity": true,
      "volumeLifecycleModes": [
        "Persistent"
      ]
    }
  }
]