[
  {
    "apiVersion": "policy/v1beta1",
    "kind": "PodDisruptionBudget",
    "metadata": {
      "name": "release-name-redirect"
    },
    "spec": {
      "minAvailable": 1,
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "redirect",
          "app.kubernetes.io/instance": "release-name"
        }
      }
    }
  },
  {
    "apiVersion": "v1",
    "kind": "ServiceAccount",
    "metadata": {
      "name": "release-name-redirect",
      "labels": {
        "app": "redirect",
        "app.kubernetes.io/instance": "redirect-0.10.1",
        "app.kubernetes.io/name": "redirect",
        "app.kubernetes.io/version": "0.10.1",
        "appRevision": "redirect___1",
        "chart": "redirect-0.10.1",
        "helm.sh/chart": "redirect-0.10.1",
        "release": "release-name",
        "releaseRevision": "1",
        "heritage": "Helm"
      }
    }
  },
  {
    "apiVersion": "v1",
    "kind": "ConfigMap",
    "metadata": {
      "name": "release-name-redirect",
      "labels": {
        "app": "redirect",
        "app.kubernetes.io/instance": "redirect-0.10.1",
        "app.kubernetes.io/name": "redirect",
        "app.kubernetes.io/version": "0.10.1",
        "appRevision": "redirect___1",
        "chart": "redirect-0.10.1",
        "helm.sh/chart": "redirect-0.10.1",
        "release": "release-name",
        "releaseRevision": "1",
        "heritage": "Helm"
      }
    },
    "data": {
      "config.yaml": "defaultFallbackTarget: https://www.wyrihaximus.net/\nenforceHttps: false\nbuildin:\n  wwwToNonWww: true\n  nonWwwToWww: false\nhosts:\n  www.ceesjankiewiet.nl: wyrihaximus.net\n"
    }
  },
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-redirect",
      "labels": {
        "app": "redirect",
        "app.kubernetes.io/instance": "redirect-0.10.1",
        "app.kubernetes.io/name": "redirect",
        "app.kubernetes.io/version": "0.10.1",
        "appRevision": "redirect___1",
        "chart": "redirect-0.10.1",
        "helm.sh/chart": "redirect-0.10.1",
        "release": "release-name",
        "releaseRevision": "1",
        "heritage": "Helm"
      }
    },
    "spec": {
      "type": "ClusterIP",
      "ports": [
        {
          "port": 7132,
          "targetPort": 7132,
          "protocol": "TCP",
          "name": "http"
        },
        {
          "port": 7133,
          "targetPort": 7133,
          "protocol": "TCP",
          "name": "http-metrics"
        }
      ],
      "selector": {
        "app.kubernetes.io/name": "redirect",
        "app.kubernetes.io/instance": "release-name"
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-redirect",
      "labels": {
        "app": "redirect",
        "app.kubernetes.io/instance": "redirect-0.10.1",
        "app.kubernetes.io/name": "redirect",
        "app.kubernetes.io/version": "0.10.1",
        "appRevision": "redirect___1",
        "chart": "redirect-0.10.1",
        "helm.sh/chart": "redirect-0.10.1",
        "release": "release-name",
        "releaseRevision": "1",
        "heritage": "Helm",
        "language": "random"
      }
    },
    "spec": {
      "replicas": 2,
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "redirect",
          "app.kubernetes.io/instance": "release-name"
        }
      },
      "template": {
        "metadata": {
          "annotations": {
            "prometheus.io/scrape": "true",
            "prometheus.io/port": "7133"
          },
          "labels": {
            "app": "redirect",
            "app.kubernetes.io/instance": "release-name",
            "app.kubernetes.io/name": "redirect",
            "app.kubernetes.io/version": "0.10.1",
            "appRevision": "redirect___1",
            "chart": "redirect-0.10.1",
            "helm.sh/chart": "redirect-0.10.1",
            "release": "release-name",
            "releaseRevision": "1",
            "heritage": "Helm",
            "language": "random"
          }
        },
        "spec": {
          "serviceAccountName": "release-name-redirect",
          "securityContext": {},
          "containers": [
            {
              "name": "redirect",
              "securityContext": {
                "allowPrivilegeEscalation": false,
                "capabilities": {
                  "drop": {
                    "": "NET_RAW"
                  }
                },
                "readOnlyRootFilesystem": true,
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              },
              "image": "ghcr.io/wyrihaximusnet/redirect:random",
              "imagePullPolicy": "Always",
              "ports": [
                {
                  "name": "http",
                  "containerPort": 7132,
                  "protocol": "TCP"
                },
                {
                  "name": "http-metrics",
                  "containerPort": 7133,
                  "protocol": "TCP"
                }
              ],
              "livenessProbe": {
                "httpGet": {
                  "path": "/ping",
                  "port": "http-metrics"
                }
              },
              "readinessProbe": {
                "httpGet": {
                  "path": "/ping",
                  "port": "http-metrics"
                }
              },
              "startupProbe": {
                "failureThreshold": 33,
                "periodSeconds": 1,
                "httpGet": {
                  "path": "/ping",
                  "port": "http-metrics"
                }
              },
              "volumeMounts": [
                {
                  "name": "release-name-redirect-volume",
                  "mountPath": "/etc/redirect/"
                }
              ],
              "resources": {
                "limits": {
                  "cpu": "50m",
                  "memory": "64Mi"
                },
                "requests": {
                  "cpu": "50m",
                  "memory": "64Mi"
                }
              }
            }
          ],
          "volumes": [
            {
              "name": "release-name-redirect-volume",
              "configMap": {
                "name": "release-name-redirect"
              }
            }
          ],
          "affinity": {
            "podAntiAffinity": {
              "requiredDuringSchedulingIgnoredDuringExecution": [
                {
                  "labelSelector": {
                    "matchExpressions": [
                      {
                        "key": "appRevision",
                        "operator": "In",
                        "values": [
                          "redirect___1"
                        ]
                      }
                    ]
                  },
                  "topologyKey": "kubernetes.io/hostname"
                }
              ]
            }
          }
        }
      }
    }
  },
  {
    "apiVersion": "networking.k8s.io/v1",
    "kind": "Ingress",
    "metadata": {
      "name": "release-name-redirect-www-wyrihaximus-net",
      "labels": {
        "app": "redirect",
        "app.kubernetes.io/instance": "redirect-0.10.1",
        "app.kubernetes.io/name": "redirect",
        "app.kubernetes.io/version": "0.10.1",
        "appRevision": "redirect___1",
        "chart": "redirect-0.10.1",
        "helm.sh/chart": "redirect-0.10.1",
        "release": "release-name",
        "releaseRevision": "1",
        "heritage": "Helm"
      },
      "annotations": {
        "kubernetes.io/ingress.class": "nginx",
        "kubernetes.io/tls-acme": "true"
      }
    },
    "spec": {
      "tls": [
        {
          "hosts": [
            "www.wyrihaximus.net"
          ],
          "secretName": "tls-www-wyrihaximus-net"
        }
      ],
      "rules": [
        {
          "host": "www.wyrihaximus.net",
          "http": {
            "paths": [
              {
                "path": "/",
                "pathType": "Prefix",
                "backend": {
                  "service": {
                    "name": "release-name-redirect",
                    "port": {
                      "number": 7132
                    }
                  }
                }
              }
            ]
          }
        }
      ]
    }
  },
