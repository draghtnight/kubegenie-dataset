              ],
              "livenessProbe": {
                "httpGet": {
                  "port": "main"
                },
                "initialDelaySeconds": 5,
                "periodSeconds": 10
              },
              "readinessProbe": {
                "httpGet": {
                  "port": "main"
                },
                "initialDelaySeconds": 5,
                "periodSeconds": 10
              },
              "volumeMounts": [
                {
                  "name": "data",
                  "mountPath": "/data"
                }
              ],
              "resources": {
