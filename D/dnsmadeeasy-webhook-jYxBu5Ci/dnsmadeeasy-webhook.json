      },
      "annotations": null
    },
    "spec": {
      "type": "ClusterIP",
      "ports": [
        {
          "port": 443,
          "targetPort": 4443,
          "protocol": "TCP",
          "name": "https"
        }
      ],
      "selector": {
        "app.kubernetes.io/name": "dnsmadeeasy-webhook",
        "app.kubernetes.io/instance": "release-name"
      }
