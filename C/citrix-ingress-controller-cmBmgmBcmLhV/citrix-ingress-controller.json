  {
    "kind": "ClusterRoleBinding",
    "apiVersion": "rbac.authorization.k8s.io/v1",
    "metadata": {
      "name": "release-name-citrix-ingress-controller"
    },
    "roleRef": {
      "apiGroup": "rbac.authorization.k8s.io",
      "kind": "ClusterRole",
      "name": "release-name-citrix-ingress-controller"
    },
    "subjects": [
      {
        "kind": "ServiceAccount",
        "name": "release-name-citrix-ingress-controller",
        "namespace": "default"
      }
    ]
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-citrix-ingress-controller",
      "namespace": "cmBmgmBcmLhV"
    },
    "spec": {
      "selector": {
        "matchLabels": {
          "app": "release-name-citrix-ingress-controller"
        }
      },
      "replicas": 1,
      "template": {
        "metadata": {
          "name": "cic",
          "labels": {
            "app": "release-name-citrix-ingress-controller"
          }
        },
        "spec": {
          "serviceAccountName": "release-name-citrix-ingress-controller",
          "containers": [
            {
              "name": "cic",
              "image": "quay.io/citrix/citrix-k8s-ingress-controller:1.25.6",
              "imagePullPolicy": "IfNotPresent",
              "args": [
                "--configmap default/release-name-cic-configmap",
                "--feature-node-watch false",
                "--enable-cnc-pbr false",
                "--update-ingress-status yes"
              ],
              "env": [
                {
                  "name": "NS_IP",
                  "value": "x.x.x.x"
                },
                {
                  "name": "NS_USER",
                  "valueFrom": {
                    "secretKeyRef": {
                      "name": null,
                      "key": "username"
                    }
                  }
                },
                {
                  "name": "NS_PASSWORD",
                  "valueFrom": {
                    "secretKeyRef": {
                      "name": null,
                      "key": "password"
                    }
                  }
                },
                {
                  "name": "EULA",
                  "value": "false"
                },
                {
                  "name": "NS_APPS_NAME_PREFIX",
                  "value": "k8s"
                }
              ],
              "resources": {
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              }
            }
          ]
        }
      }
    }
  }
]