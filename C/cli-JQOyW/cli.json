[
  {
    "apiVersion": "v1",
    "kind": "Secret",
    "metadata": {
      "name": "ocomp-pem",
      "namespace": "JQOyW",
      "labels": {
        "app": "cli",
        "chart": "cli-6.0.0",
        "release": "release",
        "heritage": "Helm"
      }
    },
    "type": "Opaque",
    "data": {
      "ocomp.pem": ""
    }
  },
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "cli",
      "namespace": "JQOyW",
      "labels": {
        "app": "cli",
        "chart": "cli-6.0.0",
        "release": "release",
        "heritage": "Helm"
      }
    },
    "spec": {
      "type": "NodePort",
      "ports": [
        {
          "port": 443,
          "targetPort": 443,
          "nodePort": 30260,
          "name": "cli443"
        },
        {
          "port": 9090,
          "targetPort": 9090,
          "nodePort": 30271,
          "name": "cli9090"
        }
      ],
      "selector": {
        "app": "cli",
        "release": "release"
      }
    }
  },
  {
    "apiVersion": "extensions/v1beta1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-cli",
      "namespace": "JQOyW",
      "labels": {
        "app": "cli",
        "chart": "cli-6.0.0",
        "release": "release",
        "heritage": "Helm"
      }
    },
    "spec": {
      "replicas": 1,
      "template": {
        "metadata": {
          "labels": {
            "app": "cli",
            "release": "release"
          }
        },
        "spec": {
          "containers": [
            {
              "name": "cli",
              "image": "nexus3.onap.org:10001/onap/cli:5.0.4",
              "imagePullPolicy": "Always",
              "ports": [
                {
                  "containerPort": 443
                },
                {
                  "containerPort": 9090
                }
              ],
              "livenessProbe": {
                "tcpSocket": {
                  "port": 443
                },
                "initialDelaySeconds": 10,
                "periodSeconds": 10
              },
              "readinessProbe": {
                "tcpSocket": {
                  "port": 443
                },
                "initialDelaySeconds": 10,
                "periodSeconds": 10
              },
              "volumeMounts": [
                {
                  "name": "ocomp-pem",
                  "mountPath": "/etc/lighttpd/ocomp.pem",
                  "subPath": "ocomp.pem",
                  "readOnly": true
                }
              ],
              "env": [
                {
                  "name": "OPEN_CLI_MODE",
                  "value": "daemon"
                }
              ],
              "resources": {
                "limits": {
                  "cpu": 1,
                  "memory": "2Gi"
                },
                "requests": {
                  "cpu": "10m",
                  "memory": "500Mi"
                }
              }
            }
          ],
          "volumes": [
            {
              "name": "ocomp-pem",
              "secret": {
                "secretName": "ocomp-pem"
              }
            }
          ],
          "imagePullSecrets": [
            {
              "name": "default-docker-registry-key"
            }
          ]
        }
      }
    }
  }
]