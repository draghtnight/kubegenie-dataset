[
  {
    "apiVersion": "v1",
    "kind": "ServiceAccount",
    "metadata": {
      "name": "release-name-cso-proxy",
      "labels": {
        "helm.sh/chart": "cso-proxy-0.1.3",
        "app.kubernetes.io/name": "cso-proxy",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "cccf49fd",
        "app.kubernetes.io/managed-by": "Helm"
      }
    }
  },
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-cso-proxy",
      "labels": {
        "helm.sh/chart": "cso-proxy-0.1.3",
        "app.kubernetes.io/name": "cso-proxy",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "cccf49fd",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "type": "ClusterIP",
      "ports": [
        {
          "port": 8080,
          "targetPort": "http",
          "protocol": "TCP",
          "name": "http"
        }
      ],
      "selector": {
        "app.kubernetes.io/name": "cso-proxy",
        "app.kubernetes.io/instance": "release-name"
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-cso-proxy",
      "labels": {
        "helm.sh/chart": "cso-proxy-0.1.3",
        "app.kubernetes.io/name": "cso-proxy",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "cccf49fd",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "cso-proxy",
          "app.kubernetes.io/instance": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app.kubernetes.io/name": "cso-proxy",
            "app.kubernetes.io/instance": "release-name"
          }
        },
        "spec": {
          "serviceAccountName": "release-name-cso-proxy",
          "securityContext": {},
          "containers": [
            {
              "name": "cso-proxy",
              "securityContext": {
                "allowPrivilegeEscalation": false,
                "capabilities": {
                  "drop": [
                    "ALL"
                  ]
                },
                "runAsNonRoot": true,
                "runAsUser": 1000,
                "seccompProfile": {
                  "type": "RuntimeDefault"
                },
                "readOnlyRootFilesystem": true
              },
              "image": "ghcr.io/djcass44/cso-proxy:cccf49fd",
              "imagePullPolicy": "IfNotPresent",
              "env": [
                {
                  "name": "PORT",
                  "value": "8080"
                }
              ],
              "ports": [
                {
                  "name": "http",
                  "containerPort": 8080
                }
              ],
              "resources": {
                "requests": {
                  "cpu": "100m"
                },
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              },
              "livenessProbe": {
                "httpGet": {
                  "port": "http",
                  "path": "/healthz"
                },
                "initialDelaySeconds": 10,
                "periodSeconds": 10,
                "timeoutSeconds": 5,
                "failureThreshold": 6,
                "successThreshold": 1
              },
              "readinessProbe": {
                "httpGet": {
                  "port": "http",
                  "path": "/healthz"
                },
                "initialDelaySeconds": 5,
                "periodSeconds": 5,
                "timeoutSeconds": 3,
                "failureThreshold": 3,
                "successThreshold": 1
              }
            }
          ]
        }
      }
    }
  }
]