                },
                {
                  "name": "config-volume",
                  "mountPath": "/config/config.yaml",
                  "subPath": "config.yaml"
                },
                {
                  "name": "analyzer-scratch",
                  "mountPath": "/analysis_scratch"
                }
              ],
              "livenessProbe": {
                "httpGet": {
                  "path": "/health",
                  "port": "analyzer-api"
                },
                "initialDelaySeconds": 120,
                "timeoutSeconds": 10,
                "periodSeconds": 10,
                "failureThreshold": 6,
                "successThreshold": 1
              },
              "readinessProbe": {
                "httpGet": {
                  "path": "/health",
                  "port": "analyzer-api"
                },
                "timeoutSeconds": 10,
                "periodSeconds": 10,
                "failureThreshold": 3,
                "successThreshold": 1
              },
              "resources": null
            }
          ],
          "volumes": [
            {
              "name": "config-volume",
              "configMap": {
                "name": "release-name-anchore-engine"
              }
            },
            {
              "name": "analyzer-scratch",
              "emptyDir": {}
            },
            {
              "name": "analyzer-config-volume",
              "configMap": {
                "name": "release-name-anchore-engine-analyzer"
              }
            }
          ]
        }
      }
    }
