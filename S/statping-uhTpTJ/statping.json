[
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-statping",
      "labels": {
        "app": "statping",
        "release": "release-name",
        "heritage": "Helm"
      }
    },
    "spec": {
      "type": "ClusterIP",
      "ports": [
        {
          "port": 8080,
          "targetPort": 8080,
          "protocol": "TCP",
          "name": "statping"
        }
      ],
      "selector": {
        "app": "statping",
        "release": "release-name"
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-statping",
      "labels": {
        "chart": "statping-0.0.3",
        "app": "statping",
        "release": "release-name",
        "heritage": "Helm"
      }
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app": "statping",
          "release": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app": "statping",
            "release": "release-name"
          }
        },
        "spec": {
          "volumes": [
            {
              "name": "release-name-statping-storage",
              "emptyDir": {}
            }
          ],
          "containers": [
            {
              "name": "statping",
              "image": "statping/statping:v0.90.74",
              "imagePullPolicy": "IfNotPresent",
              "ports": [
                {
                  "name": "http",
                  "containerPort": 8080
                }
              ],
              "env": [
                {
                  "name": "DB_DATABASE",
                  "value": "statup"
                },
                {
                  "name": "DB_HOST",
                  "value": "postgres"
                },
                {
                  "name": "DB_PASS",
                  "value": "password123"
                },
                {
                  "name": "DB_USER",
                  "value": "statup"
                },
                {
                  "name": "DESCRIPTION",
                  "value": "This is a Statping instance"
                },
                {
                  "name": "NAME",
                  "value": "STATPING"
                },
                {
                  "name": "VIRTUAL_HOST",
                  "value": "localhost"
                },
                {
                  "name": "VIRTUAL_PORT",
                  "value": "8080"
                }
              ],
              "resources": {
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              },
              "volumeMounts": [
                {
                  "name": "release-name-statping-storage",
                  "mountPath": "/app",
                  "readOnly": false
                }
              ]
            }
          ]
        }
      }
    }
  },
  {
    "kind": "Ingress",
    "metadata": {
      "name": "release-name-statping",
      "labels": {
        "app": "statping",
        "chart": "statping-0.0.3",
        "heritage": "Helm",
        "release": "release-name"
      }
    },
    "spec": {
      "rules": null
    }
  }
]