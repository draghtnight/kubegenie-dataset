      }
    },
    "spec": {
      "replicas": 1,
      "strategy": {
        "rollingUpdate": {
          "maxSurge": 0,
          "maxUnavailable": 1
        },
        "type": "RollingUpdate"
      },
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "kubernetes-dashboard",
          "app.kubernetes.io/instance": "release-name",
          "app.kubernetes.io/component": "kubernetes-dashboard"
        }
      },
