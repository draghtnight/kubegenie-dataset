                  "containerPort": 8001,
                  "protocol": "TCP"
                }
              ],
              "livenessProbe": {
                "httpGet": {
                  "path": "/en/login",
                  "port": "http",
                  "httpHeaders": [
                    {
                      "name": "Host",
                      "value": "localhost"
                    }
                  ]
                },
                "periodSeconds": 10
              },
              "readinessProbe": {
                "httpGet": {
                  "path": "/en/login",
                  "port": "http",
                  "httpHeaders": [
                    {
                      "name": "Host",
                      "value": "localhost"
                    }
                  ]
                },
                "periodSeconds": 10
              },
              "resources": {
                "limits": {
                  "cpu": "1000m",
                  "memory": "512Mi"
                },
                "requests": {
                  "cpu": "500m",
                  "memory": "256Mi"
                }
              },
              "volumeMounts": [
                {
                  "mountPath": "/opt/kimai/var",
                  "name": "kimai-data"
                },
                {
                  "name": "config",
                  "mountPath": "/opt/kimai/config/packages/local.yaml",
                  "subPath": "local.yaml"
                }
              ]
            }
          ],
          "volumes": [
            {
              "name": "kimai-data",
              "persistentVolumeClaim": {
                "claimName": "release-name-kimai2"
              }
            },
            {
              "name": "config",
              "configMap": {
                "name": "kimai2-config"
              }
            }
          ]
        }
      }
