        "spec": {
          "securityContext": {
            "fsGroup": 33
          },
          "containers": [
            {
              "name": "kimai2",
              "securityContext": {
                "allowPrivilegeEscalation": false,
                "capabilities": {
                  "drop": {
                    "": "NET_RAW"
                  }
                },
                "readOnlyRootFilesystem": true,
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              },
              "image": "kimai/kimai2:apache-1.15.6-prod",
              "imagePullPolicy": "IfNotPresent",
