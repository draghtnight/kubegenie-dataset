            "app.kubernetes.io/instance": "release-name"
          }
        },
        "spec": {
          "serviceAccountName": "release-name-kube-web-view",
          "containers": [
            {
              "name": "release-name-kube-web-view",
              "image": "hjacobs/kube-web-view:20.10.0",
              "args": [
                "--port=8080"
              ],
              "envFrom": [
                {
                  "configMapRef": {
                    "name": "release-name-kube-web-view-config"
                  }
                },
                {
                  "secretRef": {
                    "name": "release-name-kube-web-view-config"
                  }
                }
              ],
              "ports": [
                {
                  "containerPort": 8080
                }
              ],
              "readinessProbe": {
                "httpGet": {
                  "path": "/health",
                  "port": 8080
                }
              },
              "securityContext": {
                "readOnlyRootFilesystem": true,
                "runAsNonRoot": true,
                "runAsUser": 11748,
                "allowPrivilegeEscalation": false,
                "capabilities": {
                  "drop": {
                    "": "NET_RAW"
                  }
                }
              },
              "resources": {
                "limits": {
                  "memory": "100Mi"
                },
                "requests": {
                  "cpu": "5m",
                  "memory": "100Mi"
                },
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              }
            }
          ]
        }
      }
    }
