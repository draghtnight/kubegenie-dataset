[
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "annotations": {},
      "name": "release-name-prometheus-nats-exporter",
      "labels": {
        "app.kubernetes.io/name": "prometheus-nats-exporter",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/managed-by": "Helm",
        "helm.sh/chart": "prometheus-nats-exporter-2.9.3"
      }
    },
    "spec": {
      "type": "ClusterIP",
      "ports": [
        {
          "port": 80,
          "targetPort": 7777,
          "protocol": "TCP",
          "name": "http"
        }
      ],
      "selector": {
        "app.kubernetes.io/name": "prometheus-nats-exporter",
        "app.kubernetes.io/instance": "release-name"
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-prometheus-nats-exporter",
      "labels": {
        "app.kubernetes.io/name": "prometheus-nats-exporter",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/managed-by": "Helm",
        "helm.sh/chart": "prometheus-nats-exporter-2.9.3"
      }
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "prometheus-nats-exporter",
          "app.kubernetes.io/instance": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app.kubernetes.io/name": "prometheus-nats-exporter",
            "app.kubernetes.io/instance": "release-name"
          },
          "annotations": null
        },
        "spec": {
          "containers": [
            {
              "name": "prometheus-nats-exporter",
              "args": [
                "-port",
                "7777",
                "-varz",
                "-channelz",
                "-connz",
                "-routez",
                "-serverz",
                "-subz",
                "-gatewayz",
                "-jsz=all",
                "-leafz",
                "http://nats-nats-monitoring.default.svc:8222"
              ],
              "image": "natsio/prometheus-nats-exporter:0.9.3",
              "imagePullPolicy": "IfNotPresent",
              "ports": [
                {
                  "name": "http",
                  "containerPort": 7777,
                  "protocol": "TCP"
                }
              ],
              "livenessProbe": {
                "httpGet": {
                  "path": "/metrics",
                  "port": "http"
                }
              },
              "readinessProbe": {
                "httpGet": {
                  "path": "/metrics",
                  "port": "http"
                }
              },
              "resources": {
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              }
            }
          ]
        }
      }
    }
  }
]