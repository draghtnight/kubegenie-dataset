        "helm.sh/chart": "powerdnsadmin-0.3.1",
        "app.kubernetes.io/name": "powerdnsadmin",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "0.2.3",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "powerdnsadmin",
          "app.kubernetes.io/instance": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app.kubernetes.io/name": "powerdnsadmin",
            "app.kubernetes.io/instance": "release-name"
          },
          "annotations": {
