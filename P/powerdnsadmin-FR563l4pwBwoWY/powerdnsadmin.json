          }
        },
        "spec": {
          "serviceAccountName": "release-name-powerdnsadmin",
          "securityContext": {},
          "containers": [
            {
              "name": "powerdnsadmin",
              "securityContext": {
                "allowPrivilegeEscalation": false,
                "capabilities": {
                  "drop": {
                    "": "NET_RAW"
                  }
                },
                "readOnlyRootFilesystem": true
              },
              "image": "ngoduykhanh/powerdns-admin:0.2.3",
              "imagePullPolicy": "IfNotPresent",
              "env": [
                {
                  "name": "SQLALCHEMY_DATABASE_URI",
                  "valueFrom": {
                    "secretKeyRef": {
                      "name": "release-name-powerdnsadmin",
                      "key": "SQLALCHEMY_DATABASE_URI"
                    }
                  }
                }
              ],
              "ports": [
                {
                  "name": "http",
                  "containerPort": 80,
                  "protocol": "TCP"
                }
              ],
              "livenessProbe": {
                "httpGet": {
                  "path": "/",
                  "port": "http"
                },
                "failureThreshold": 5,
                "periodSeconds": 10
              },
              "readinessProbe": {
                "httpGet": {
                  "path": "/",
                  "port": "http"
                },
                "failureThreshold": 5,
                "periodSeconds": 10
              },
              "startupProbe": {
                "httpGet": {
                  "path": "/",
                  "port": "http"
                },
                "initialDelaySeconds": 5,
                "failureThreshold": 30,
                "periodSeconds": 10
              },
              "resources": {
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              }
            }
          ]
        }
      }
    }
