                  "path": "/healthz",
                  "port": "https",
                  "scheme": "HTTPS"
                },
                "initialDelaySeconds": 30,
                "timeoutSeconds": 5
              },
              "securityContext": {
                "allowPrivilegeEscalation": false,
                "capabilities": {
                  "drop": [
                    "all"
                  ]
                },
                "readOnlyRootFilesystem": true,
                "runAsNonRoot": true,
                "runAsUser": 10001
              },
              "volumeMounts": [
                {
                  "mountPath": "/etc/adapter/",
                  "name": "config",
                  "readOnly": true
                },
                {
                  "mountPath": "/tmp",
                  "name": "tmp"
                }
              ]
            }
