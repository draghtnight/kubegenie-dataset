  {
    "apiVersion": "batch/v1",
    "kind": "Job",
    "metadata": {
      "name": "release-name-cleaner",
      "labels": {
        "chart": "git-apiserver-0.1.0",
        "app": "git-apiserver",
        "heritage": "Helm",
        "release": "release-name"
      },
      "annotations": {
        "helm.sh/hook": "pre-delete",
        "helm.sh/hook-delete-policy": "hook-succeeded,hook-failed"
      }
    },
    "spec": {
      "backoffLimit": 3,
      "activeDeadlineSeconds": 120,
      "template": {
        "spec": {
          "serviceAccountName": "release-name-git-apiserver",
          "containers": [
            {
              "name": "busybox",
              "image": "appscode/kubectl:v1.11",
              "command": [
                "sh",
                "-c",
                "sleep 2; kubectl delete validatingwebhookconfigurations admission.git.kube.ci || true"
              ],
              "imagePullPolicy": "IfNotPresent"
            }
          ],
          "restartPolicy": "Never"
        }
      }
    }
  },
  {
    "apiVersion": "admissionregistration.k8s.io/v1beta1",
    "kind": "ValidatingWebhookConfiguration",
    "metadata": {
      "name": "admission.git.kube.ci",
      "labels": {
        "chart": "git-apiserver-0.1.0",
        "app": "git-apiserver",
        "heritage": "Helm",
        "release": "release-name"
      },
      "annotations": {
        "helm.sh/hook": "post-install,post-upgrade",
        "helm.sh/hook-delete-policy": "before-hook-creation"
      }
    },
    "webhooks": [
      {
        "name": "repository.admission.git.kube.ci",
        "clientConfig": {
          "service": {
            "namespace": "default",
            "name": "kubernetes",
            "path": "/apis/admission.git.kube.ci/v1alpha1/repositories"
          },
          "caBundle": "bm90LWNhLWNlcnQ="
        },
        "rules": [
          {
            "operations": [
              "CREATE",
              "UPDATE"
            ],
            "apiGroups": [
              "git.kube.ci"
            ],
            "apiVersions": [
              "*"
            ],
            "resources": [
              "repositories"
            ]
          }
        ],
        "failurePolicy": "Fail"
      }
    ]
  }
