[
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-apache",
      "labels": {
        "chart": "apache-0.1.3"
      }
    },
    "spec": {
      "type": "ClusterIP",
      "ports": [
        {
          "port": 80,
          "targetPort": 80,
          "protocol": "TCP",
          "name": "apache"
        }
      ],
      "selector": {
        "app": "release-name-apache"
      }
    }
  },
  {
    "apiVersion": "extensions/v1beta1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-apache",
      "labels": {
        "chart": "apache-0.1.3"
      }
    },
    "spec": {
      "replicas": 1,
      "template": {
        "metadata": {
          "labels": {
            "app": "release-name-apache"
          }
        },
        "spec": {
          "containers": [
            {
              "name": "apache",
              "image": "cloudposse/apache:latest",
              "imagePullPolicy": "Always",
              "env": [
                {
                  "name": "APACHE_EVENT_MAX_REQUEST_WORKERS",
                  "value": "150"
                },
                {
                  "name": "APACHE_EVENT_MAX_SPARE_THREADS",
                  "value": "75"
                },
                {
                  "name": "APACHE_EVENT_MIN_SPARE_THREADS",
                  "value": "25"
                },
                {
                  "name": "APACHE_EVENT_START_SERVERS",
                  "value": "2"
                },
                {
                  "name": "APACHE_EVENT_THREADS_PER_CHILD",
                  "value": "25"
                },
                {
                  "name": "APACHE_EVENT_THREAD_LIMIT",
                  "value": "64"
                },
                {
                  "name": "APACHE_SERVER_NAME",
                  "value": "localhost"
                },
                {
                  "name": "APACHE_WORKER_MAX_REQUEST_WORKERS",
                  "value": "4"
                },
                {
                  "name": "APACHE_WORKER_MAX_SPARE_THREADS",
                  "value": "10"
                },
                {
                  "name": "APACHE_WORKER_MIN_SPARE_THREADS",
                  "value": "2"
                },
                {
                  "name": "APACHE_WORKER_START_SERVERS",
                  "value": "2"
                },
                {
                  "name": "APACHE_WORKER_THREADS_PER_CHILD",
                  "value": "25"
                },
                {
                  "name": "APACHE_WORKER_THREAD_LIMIT",
                  "value": "64"
                },
                {
                  "name": "PHP_FPM_MAX_CHILDREN",
                  "value": "10"
                },
                {
                  "name": "PHP_FPM_MAX_REQUESTS",
                  "value": "500"
                },
                {
                  "name": "PHP_FPM_MAX_SPARE_SERVERS",
                  "value": "10"
                },
                {
                  "name": "PHP_FPM_PM",
                  "value": "ondemand"
                },
                {
                  "name": "PHP_FPM_PROCESS_IDLE_TIMEOUT",
                  "value": "25s"
                },
                {
                  "name": "PHP_FPM_SPARE_SERVERS",
                  "value": "1"
                },
                {
                  "name": "PHP_FPM_START_SERVERS",
                  "value": "1"
                }
              ],
              "ports": [
                {
                  "containerPort": 80
                }
              ],
              "readinessProbe": {
                "successThreshold": 1,
                "failureThreshold": 3,
                "httpGet": {
                  "path": "/",
                  "port": 80,
                  "scheme": "HTTP",
                  "httpHeaders": [
                    {
                      "name": "Host",
                      "value": "localhost"
                    }
                  ]
                },
                "periodSeconds": 10,
                "timeoutSeconds": 1
              },
              "livenessProbe": {
                "successThreshold": 1,
                "failureThreshold": 3,
                "httpGet": {
                  "path": "/",
                  "port": 80,
                  "scheme": "HTTP",
                  "httpHeaders": [
                    {
                      "name": "Host",
                      "value": "localhost"
                    }
                  ]
                },
                "periodSeconds": 10,
                "timeoutSeconds": 1
              },
              "resources": {
                "limits": {
                  "cpu": "100m",
                  "memory": "128Mi"
                },
                "requests": {
                  "cpu": "100m",
                  "memory": "128Mi"
                }
              },
              "volumeMounts": null
            }
          ],
          "volumes": null
        }
      }
    }
  }
]