[
  {
    "apiVersion": "v1",
    "kind": "ConfigMap",
    "metadata": {
      "name": "release-name-activation-service",
      "labels": {
        "app.kubernetes.io/name": "activation-service",
        "helm.sh/chart": "activation-service-0.2.1",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "0.2.0",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "data": {
      "as.yml": "ar:\n  delegation: https://ar.packetdelivery.net/delegation\n  id: EU.EORI.NLPACKETDEL\n  policy: https://ar.packetdelivery.net/policy\n  rejectUnauthorized: false\n  token: https://ar.packetdelivery.net/connect/token\nclient:\n  crt: <pdc-certs>\n  id: EU.EORI.NLPACKETDEL\n  key: <pdc-private-key>\ndb:\n  source: ':memory:'\nexpress:\n  port: 7000"
    }
  },
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-activation-service",
      "labels": {
        "app.kubernetes.io/name": "activation-service",
        "helm.sh/chart": "activation-service-0.2.1",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "0.2.0",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "type": "ClusterIP",
      "ports": [
        {
          "port": 80,
          "targetPort": 7000,
          "protocol": "TCP",
          "name": "http"
        }
      ],
      "selector": {
        "app.kubernetes.io/name": "activation-service",
        "app.kubernetes.io/instance": "release-name"
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-activation-service",
      "labels": {
        "app.kubernetes.io/name": "activation-service",
        "helm.sh/chart": "activation-service-0.2.1",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "0.2.0",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "replicas": 1,
      "revisionHistoryLimit": 3,
      "strategy": {
        "rollingUpdate": {
          "maxSurge": 1,
          "maxUnavailable": 0
        },
        "type": "RollingUpdate"
      },
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "activation-service",
          "app.kubernetes.io/instance": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app.kubernetes.io/name": "activation-service",
            "helm.sh/chart": "activation-service-0.2.1",
            "app.kubernetes.io/instance": "release-name",
            "app.kubernetes.io/version": "0.2.0",
            "app.kubernetes.io/managed-by": "Helm"
          }
        },
        "spec": {
          "serviceAccountName": "default",
          "containers": [
            {
              "name": "activation-service",
              "imagePullPolicy": "IfNotPresent",
              "image": "i4trust/activation-service:v0.2.0",
              "ports": [
                {
                  "name": "http",
                  "containerPort": 7000,
                  "protocol": "TCP"
                }
              ],
              "livenessProbe": {
                "httpGet": {
                  "path": "/health",
                  "port": 7000
                },
                "initialDelaySeconds": 20,
                "periodSeconds": 10,
                "successThreshold": 1,
                "timeoutSeconds": 30
              },
              "readinessProbe": {
                "httpGet": {
                  "path": "/health",
                  "port": 7000
                },
                "initialDelaySeconds": 21,
                "periodSeconds": 10,
                "successThreshold": 1,
                "timeoutSeconds": 30
              },
              "resources": null,
              "volumeMounts": [
                {
                  "name": "config-volume",
                  "mountPath": "/home/aservice/config/as.yml",
                  "subPath": "as.yml"
                }
              ]
            }
          ],
          "volumes": [
            {
              "name": "config-volume",
              "configMap": {
                "name": "release-name-activation-service",
                "items": [
                  {
                    "key": "as.yml",
                    "path": "as.yml"
                  }
                ]
              }
            }
          ]
        }
      }
    }
  }
]