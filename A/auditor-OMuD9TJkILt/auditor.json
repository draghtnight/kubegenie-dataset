        },
        "spec": {
          "serviceAccountName": "release-name-auditor",
          "containers": [
            {
              "name": "operator",
              "securityContext": {
                "allowPrivilegeEscalation": false,
                "capabilities": {
                  "drop": {
                    "": "NET_RAW"
