[
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-audacity",
      "labels": {
        "helm.sh/chart": "audacity-0.1.4",
        "app.kubernetes.io/name": "audacity",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "3.0.2",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "type": "ClusterIP",
      "ports": [
        {
          "port": 3000,
          "targetPort": "http",
          "protocol": "TCP",
          "name": "http"
        }
      ],
      "selector": {
        "app.kubernetes.io/name": "audacity",
        "app.kubernetes.io/instance": "release-name"
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-audacity",
      "labels": {
        "helm.sh/chart": "audacity-0.1.4",
        "app.kubernetes.io/name": "audacity",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "3.0.2",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "replicas": 1,
      "strategy": {
        "type": "RollingUpdate"
      },
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "audacity",
          "app.kubernetes.io/instance": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app.kubernetes.io/name": "audacity",
            "app.kubernetes.io/instance": "release-name"
          }
        },
        "spec": {
          "serviceAccountName": "default",
          "dnsPolicy": "ClusterFirst",
          "containers": [
            {
              "name": "release-name-audacity",
              "image": "ghcr.io/linuxserver/audacity:version-3.0.2",
              "imagePullPolicy": "IfNotPresent",
              "ports": [
                {
                  "name": "http",
                  "containerPort": 3000,
                  "protocol": "TCP"
                }
              ],
              "volumeMounts": null,
              "livenessProbe": {
                "tcpSocket": {
                  "port": "http"
                },
                "initialDelaySeconds": 30,
                "failureThreshold": 5,
                "timeoutSeconds": 10
              },
              "readinessProbe": {
                "tcpSocket": {
                  "port": "http"
                },
                "initialDelaySeconds": 30,
                "failureThreshold": 5,
                "timeoutSeconds": 10
              }
            }
          ]
        }
      }
    }
  }
]