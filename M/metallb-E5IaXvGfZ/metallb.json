  {
    "apiVersion": "apps/v1",
    "kind": "DaemonSet",
    "metadata": {
      "name": "release-name-metallb-speaker",
      "labels": {
        "heritage": "Helm",
        "release": "release-name",
        "chart": "metallb-0.12.3",
        "app": "metallb",
        "component": "speaker"
      }
    },
    "spec": {
      "selector": {
        "matchLabels": {
          "app": "metallb",
          "component": "speaker",
          "release": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "heritage": "Helm",
            "release": "release-name",
            "chart": "metallb-0.12.3",
            "app": "metallb",
            "component": "speaker"
          }
        },
        "spec": {
          "serviceAccountName": "release-name-metallb-speaker",
          "terminationGracePeriodSeconds": 0,
          "hostNetwork": true,
          "containers": [
            {
              "name": "speaker",
              "image": "metallb/speaker:v0.8.1",
              "imagePullPolicy": "IfNotPresent",
              "args": [
                "--port=7472",
                "--config=metallb-config"
              ],
              "env": [
                {
                  "name": "METALLB_NODE_NAME",
                  "valueFrom": {
                    "fieldRef": {
                      "fieldPath": "spec.nodeName"
                    }
                  }
                },
                {
                  "name": "METALLB_HOST",
                  "valueFrom": {
                    "fieldRef": {
                      "fieldPath": "status.hostIP"
                    }
                  }
                }
              ],
              "ports": [
                {
                  "name": "monitoring",
                  "containerPort": 7472
                }
              ],
              "resources": {},
              "securityContext": {
                "allowPrivilegeEscalation": false,
                "readOnlyRootFilesystem": true,
                "capabilities": {
                  "drop": [
                    "ALL"
                  ],
                  "add": [
                    "NET_ADMIN",
                    "NET_RAW",
                    "SYS_ADMIN"
                  ]
                },
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              }
            }
          ],
          "nodeSelector": {
            "beta.kubernetes.io/os": "linux"
          },
          "tolerations": [
            {
              "key": "node-role.kubernetes.io/master",
              "effect": "NoSchedule"
            }
          ]
        }
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-metallb-controller",
      "labels": {
