[
  {
    "apiVersion": "v1",
    "kind": "ConfigMap",
    "metadata": {
      "name": "release-name-mosquitto",
      "labels": {
        "app.kubernetes.io/name": "mosquitto",
        "helm.sh/chart": "mosquitto-0.0.1",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "data": {
      "mosquitto.conf": "log_dest stdout\nlistener 1883\nlistener 9090\nprotocol websockets"
    }
  },
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-mosquitto",
      "labels": {
        "app.kubernetes.io/name": "mosquitto",
        "helm.sh/chart": "mosquitto-0.0.1",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "type": "ClusterIP",
      "ports": [
        {
          "port": 1883,
          "targetPort": "mqtt",
          "protocol": "TCP",
          "name": "mqtt"
        }
      ],
      "selector": {
        "app.kubernetes.io/name": "mosquitto",
        "app.kubernetes.io/instance": "release-name"
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-mosquitto",
      "labels": {
        "app.kubernetes.io/name": "mosquitto",
        "helm.sh/chart": "mosquitto-0.0.1",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "mosquitto",
          "app.kubernetes.io/instance": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app.kubernetes.io/name": "mosquitto",
            "app.kubernetes.io/instance": "release-name"
          }
        },
        "spec": {
          "containers": [
            {
              "name": "mosquitto",
              "image": "eclipse-mosquitto:1.6.4",
              "imagePullPolicy": "IfNotPresent",
              "volumeMounts": [
                {
                  "name": "mosquitto-config",
                  "mountPath": "/mosquitto/config"
                },
                {
                  "name": "mosquitto-data",
                  "mountPath": "/mosquitto/data"
                }
              ],
              "ports": [
                {
                  "name": "mqtt",
                  "containerPort": 1883,
                  "protocol": "TCP"
                }
              ],
              "resources": {
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              }
            }
          ],
          "volumes": [
            {
              "name": "mosquitto-config",
              "configMap": {
                "name": "release-name-mosquitto"
              }
            },
            {
              "name": "mosquitto-data",
              "emptyDir": {}
            }
          ]
        }
      }
    }
  }
]