[
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-mdb",
      "labels": {
        "app": "mdb",
        "chart": "mdb-4.0.0",
        "release": "release-name",
        "heritage": "Helm",
        "service": "release-name"
      }
    },
    "spec": {
      "type": "ClusterIP",
      "ports": [
        {
          "name": "http",
          "targetPort": "http",
          "port": 8080,
          "protocol": "TCP"
        }
      ],
      "selector": {
        "app": "mdb",
        "release": "release-name"
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-mdb",
      "labels": {
        "app": "mdb",
        "chart": "mdb-4.0.0",
        "release": "release-name",
        "heritage": "Helm",
        "version": "latest",
        "canary": "false"
      }
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app": "mdb",
          "release": "release-name",
          "canary": "false"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app": "mdb",
            "release": "release-name",
            "version": "latest",
            "canary": "false"
          }
        },
        "spec": {
          "hostNetwork": false,
          "nodeSelector": {},
          "affinity": {},
          "tolerations": [],
          "hostAliases": [],
          "containers": [
            {
              "name": "mdb",
              "image": "docker.io/bitnami/nginx:latest",
              "imagePullPolicy": "Always",
              "env": [],
              "resources": {
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              },
              "ports": [
                {
                  "name": "http",
                  "containerPort": 8080,
                  "protocol": "TCP"
                }
              ],
              "volumeMounts": null
            }
          ],
          "volumes": [
            {
              "name": "data-storage",
              "emptyDir": {}
            }
          ]
        }
      }
    }
  }
]