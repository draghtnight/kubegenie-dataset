    "roleRef": {
      "apiGroup": "rbac.authorization.k8s.io",
      "kind": "ClusterRole",
      "name": "release-name-external-dns"
    },
    "subjects": [
      {
        "kind": "ServiceAccount",
        "name": "release-name-external-dns",
        "namespace": "default"
      }
    ]
  },
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-external-dns",
      "labels": {
        "helm.sh/chart": "external-dns-1.7.0",
        "app.kubernetes.io/name": "external-dns",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "0.10.2",
        "app.kubernetes.io/managed-by": "Helm"
      },
      "annotations": {
        "prometheus.io/path": "/metrics",
        "prometheus.io/probe_module": "http-healthz",
        "prometheus.io/probe_path": "/healthz",
        "prometheus.io/scrape": "true"
      }
    },
    "spec": {
      "type": "ClusterIP",
      "selector": {
        "app.kubernetes.io/name": "external-dns",
        "app.kubernetes.io/instance": "release-name"
      },
      "ports": [
        {
          "name": "http",
          "port": 7979,
          "targetPort": "http",
          "protocol": "TCP"
        }
      ]
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-external-dns",
      "labels": {
        "helm.sh/chart": "external-dns-1.7.0",
        "app.kubernetes.io/name": "external-dns",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "0.10.2",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "external-dns",
          "app.kubernetes.io/instance": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app.kubernetes.io/name": "external-dns",
            "app.kubernetes.io/instance": "release-name"
          }
        },
        "spec": {
          "serviceAccountName": "release-name-external-dns",
          "securityContext": {
            "fsGroup": 65534
          },
          "containers": [
            {
              "name": "external-dns",
              "securityContext": {
                "capabilities": {
                  "drop": [
                    "ALL"
                  ]
                },
                "readOnlyRootFilesystem": true,
                "runAsNonRoot": true,
                "runAsUser": 65534,
                "allowPrivilegeEscalation": false
              },
              "image": "k8s.gcr.io/external-dns/external-dns:v0.10.2",
              "imagePullPolicy": "IfNotPresent",
              "env": [
                {
                  "name": "DNS_SECRET",
                  "valueFrom": {
                    "secretKeyRef": {
                      "key": "secret",
                      "name": "external-dns"
                    }
                  }
                }
              ],
              "args": [
                "--log-level=info",
                "--log-format=text",
                "--interval=1m",
                "--source=service",
                "--source=ingress",
                "--policy=upsert-only",
                "--registry=txt",
                "--provider=rfc2136",
                "--rfc2136-port=53",
                "--rfc2136-tsig-secret=$(DNS_SECRET)",
                "--rfc2136-tsig-secret-alg=hmac-sha256",
                "--rfc2136-tsig-axfr",
                "--metrics-address=:7979"
              ],
              "ports": [
                {
                  "name": "http",
                  "protocol": "TCP",
                  "containerPort": 7979
                }
              ],
              "livenessProbe": {
                "failureThreshold": 2,
                "httpGet": {
                  "path": "/healthz",
                  "port": "http"
                },
                "initialDelaySeconds": 10,
                "periodSeconds": 10,
                "successThreshold": 1,
                "timeoutSeconds": 5
              },
              "readinessProbe": {
                "failureThreshold": 6,
                "httpGet": {
                  "path": "/healthz",
                  "port": "http"
                },
                "initialDelaySeconds": 5,
                "periodSeconds": 10,
                "successThreshold": 1,
                "timeoutSeconds": 5
              },
              "resources": {
                "limits": {
                  "cpu": 1,
                  "memory": "256Mi"
                },
                "requests": {
                  "cpu": "100m",
                  "memory": "128Mi"
                }
              }
            }
          ]
        }
      }
    }
  },
  {
    "apiVersion": "v1",
    "kind": "Pod",
    "metadata": {
      "name": "release-name-external-dns-test",
      "labels": {
        "helm.sh/chart": "external-dns-0.5.0",
        "app.kubernetes.io/name": "external-dns",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "v0.10.2",
        "app.kubernetes.io/managed-by": "Helm"
      },
      "annotations": {
        "helm.sh/hook": "test"
      }
    },
    "spec": {
      "containers": [
        {
          "name": "wget",
          "image": "busybox",
          "command": [
            "wget"
          ],
          "args": [
            "-O-",
            "-S",
            "release-name-external-dns:7979/healthz"
          ]
        }
      ],
      "restartPolicy": "Never"
    }
  }
]