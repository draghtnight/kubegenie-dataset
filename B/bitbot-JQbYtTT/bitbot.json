[
  {
    "apiVersion": "v1",
    "kind": "ServiceAccount",
    "metadata": {
      "name": "release-name-bitbot",
      "labels": {
        "helm.sh/chart": "bitbot-0.1.2",
        "app.kubernetes.io/name": "bitbot",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "1.3.0",
        "app.kubernetes.io/managed-by": "Helm"
      }
    }
  },
  {
    "apiVersion": "v1",
    "kind": "PersistentVolumeClaim",
    "metadata": {
      "name": "release-name-bitbot",
      "labels": {
        "helm.sh/chart": "bitbot-0.1.2",
        "app.kubernetes.io/name": "bitbot",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "1.3.0",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "accessModes": [
        "ReadWriteOnce"
      ],
      "resources": {
        "requests": {
          "storage": "1Gi"
        }
      }
    }
  },
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-bitbot",
      "labels": {
        "helm.sh/chart": "bitbot-0.1.2",
        "app.kubernetes.io/name": "bitbot",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "1.3.0",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "type": "ClusterIP",
      "ports": [
        {
          "port": 80,
          "targetPort": "http",
          "protocol": "TCP",
          "name": "http"
        }
      ],
      "selector": {
        "app.kubernetes.io/name": "bitbot",
        "app.kubernetes.io/instance": "release-name"
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-bitbot",
      "labels": {
        "helm.sh/chart": "bitbot-0.1.2",
        "app.kubernetes.io/name": "bitbot",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "1.3.0",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "bitbot",
          "app.kubernetes.io/instance": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app.kubernetes.io/name": "bitbot",
            "app.kubernetes.io/instance": "release-name"
          }
        },
        "spec": {
          "serviceAccountName": "release-name-bitbot",
          "securityContext": {},
          "initContainers": [],
          "containers": [
            {
              "name": "bitbot",
              "securityContext": {
                "allowPrivilegeEscalation": false,
                "capabilities": {
                  "drop": {
                    "": "NET_RAW"
                  }
                },
                "readOnlyRootFilesystem": true
              },
              "image": "bbriggs/bitbot:release-v1.3.0",
              "imagePullPolicy": "IfNotPresent",
              "env": null,
              "volumeMounts": [
                {
                  "mountPath": "/tmp",
                  "name": "bbolt-db"
                }
              ],
              "resources": {
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              }
            }
          ],
          "volumes": [
            {
              "name": "bbolt-db",
              "persistentVolumeClaim": {
                "claimName": "release-name-bitbot"
              }
            }
          ]
        }
      }
    }
  },
  {
    "apiVersion": "v1",
    "kind": "Pod",
    "metadata": {
      "name": "release-name-bitbot-test-connection",
      "labels": {
        "helm.sh/chart": "bitbot-0.1.2",
        "app.kubernetes.io/name": "bitbot",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "1.3.0",
        "app.kubernetes.io/managed-by": "Helm"
      },
      "annotations": {
        "helm.sh/hook": "test-success"
      }
    },
    "spec": {
      "containers": [
        {
          "name": "wget",
          "image": "busybox",
          "command": [
            "wget"
          ],
          "args": [
            "release-name-bitbot:80"
          ]
        }
      ],
      "restartPolicy": "Never"
    }
  }
]