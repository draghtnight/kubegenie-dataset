          "securityContext": {
            "fsGroup": 1001
          },
          "containers": [
            {
              "name": "mysql",
              "image": "docker.io/bitnami/mysql:8.0.18-r70",
              "imagePullPolicy": "IfNotPresent",
              "securityContext": {
                "runAsUser": 10589,
                "allowPrivilegeEscalation": false,
                "capabilities": {
                  "drop": {
                    "": "NET_RAW"
                  }
                },
                "readOnlyRootFilesystem": true
              },
              "env": [
                {
                  "name": "BITNAMI_DEBUG",
                  "value": "false"
