[
  {
    "apiVersion": "v1",
    "kind": "ServiceAccount",
    "metadata": {
      "name": "release-name-bytebot-tip-jar",
      "labels": {
        "helm.sh/chart": "bytebot-tip-jar-0.1.1",
        "app.kubernetes.io/name": "bytebot-tip-jar",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "0.1.1",
        "app.kubernetes.io/managed-by": "Helm"
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-bytebot-tip-jar",
      "labels": {
        "helm.sh/chart": "bytebot-tip-jar-0.1.1",
        "app.kubernetes.io/name": "bytebot-tip-jar",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "0.1.1",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "bytebot-tip-jar",
          "app.kubernetes.io/instance": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app.kubernetes.io/name": "bytebot-tip-jar",
            "app.kubernetes.io/instance": "release-name"
          }
        },
        "spec": {
          "serviceAccountName": "release-name-bytebot-tip-jar",
          "securityContext": {},
          "containers": [
            {
              "name": "bytebot-tip-jar",
              "securityContext": {
                "allowPrivilegeEscalation": false,
                "capabilities": {
                  "drop": {
                    "": "NET_RAW"
                  }
                },
                "readOnlyRootFilesystem": true
              },
              "image": "ghcr.io/bytebot-chat/bytebot-tip-jar:0.1.1",
              "imagePullPolicy": null,
              "args": [
                "-redis=redis:6379",
                "-irc-inbound=irc-inbound1",
                "-irc-outbound=irc-outbound"
              ],
              "resources": {
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              }
            }
          ]
        }
      }
    }
  }
]