[
  {
    "apiVersion": "v1",
    "kind": "ServiceAccount",
    "metadata": {
      "name": "release-name-business-service",
      "labels": {
        "helm.sh/chart": "business-service-0.2.1",
        "app.kubernetes.io/name": "business-service",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "0.0.1",
        "app.kubernetes.io/managed-by": "Helm"
      }
    }
  },
  {
    "apiVersion": "v1",
    "kind": "Secret",
    "metadata": {
      "name": "release-name-business-service"
    },
    "data": {
      "POSTGRES_USER": "cG9zdGdyZXM=",
      "POSTGRES_PASSWORD": "cG9zdGdyZXM="
    }
  },
  {
    "apiVersion": "v1",
    "kind": "ConfigMap",
    "metadata": {
      "name": "release-name-business-service"
    },
    "data": {
      "POSTGRES_URI": "postgres.svc.cluster.local",
      "POSTGRES_PORT": "5432"
    }
  },
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-business-service",
      "labels": null
    },
    "spec": {
      "type": "LoadBalancer",
      "clusterIP": null,
      "ports": [
        {
          "port": 80,
          "targetPort": 8080,
          "protocol": "TCP",
          "name": "http"
        }
      ],
      "selector": null
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-business-service",
      "labels": null
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": null
      },
      "template": {
        "metadata": {
          "labels": {
            "app.kubernetes.io/name": "business-service",
            "app.kubernetes.io/instance": "release-name"
          }
        },
        "spec": {
          "serviceAccountName": "release-name-business-service",
          "securityContext": {},
          "containers": [
            {
              "name": "business-service",
              "securityContext": {
                "allowPrivilegeEscalation": false,
                "capabilities": {
                  "drop": {
                    "": "NET_RAW"
                  }
                },
                "readOnlyRootFilesystem": true
              },
              "image": "redestroyder/business-service:0.0.1",
              "imagePullPolicy": "IfNotPresent",
              "envFrom": [
                {
                  "configMapRef": {
                    "name": "release-name-business-service"
                  }
                },
                {
                  "secretRef": {
                    "name": "release-name-business-service"
                  }
                }
              ],
              "ports": [
                {
                  "name": "http",
                  "containerPort": 8080,
                  "protocol": "TCP"
                }
              ],
              "resources": {
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              }
            }
          ]
        }
      }
    }
  },
  {
    "apiVersion": "networking.istio.io/v1beta1",
    "kind": "Gateway",
    "metadata": {
      "name": "release-name-business-service",
      "labels": {
        "helm.sh/chart": "business-service-0.2.1",
        "app.kubernetes.io/name": "business-service",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "0.0.1",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "selector": null,
      "servers": [
        {
          "port": {
            "targetPort": 8080,
            "number": 80,
            "name": "http",
            "protocol": "HTTP"
          },
          "hosts": [
            "arch.homework"
          ]
        }
      ]
    }
  },
  {
    "apiVersion": "v1",
    "kind": "Pod",
    "metadata": {
      "name": "release-name-business-service-test-connection",
      "labels": {
        "helm.sh/chart": "business-service-0.2.1",
        "app.kubernetes.io/name": "business-service",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "0.0.1",
        "app.kubernetes.io/managed-by": "Helm"
      },
      "annotations": {
        "helm.sh/hook": "test"
      }
    },
    "spec": {
      "containers": [
        {
          "name": "wget",
          "image": "busybox",
          "command": [
            "wget"
          ],
          "args": [
            "release-name-business-service:80"
          ]
        }
      ],
      "restartPolicy": "Never"
    }
  }
]