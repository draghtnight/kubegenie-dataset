[
  {
    "kind": "Namespace",
    "apiVersion": "v1",
    "metadata": {
      "name": "linkerd-multicluster",
      "labels": {
        "linkerd.io/extension": "multicluster"
      }
    }
  },
  {
    "kind": "ServiceAccount",
    "apiVersion": "v1",
    "metadata": {
      "name": "linkerd-gateway",
      "namespace": "linkerd-multicluster",
      "labels": {
        "linkerd.io/extension": "multicluster"
      }
    }
  },
  {
    "apiVersion": "v1",
    "kind": "ServiceAccount",
    "metadata": {
      "name": "linkerd-service-mirror-remote-access-default",
      "namespace": "linkerd-multicluster",
      "labels": {
        "linkerd.io/extension": "multicluster"
