        "version": "0.1"
      }
    },
    "spec": {
      "dnsNames": [
        "vpn.example.com"
      ],
      "secretName": "wireguard-letsencrypt-certificate",
      "issuerRef": {
        "kind": "ClusterIssuer",
        "name": "letsencrypt"
      }
    }
  }
