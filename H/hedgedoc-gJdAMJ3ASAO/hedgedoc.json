                  "value": "hedgedoc"
                },
                {
                  "name": "DB_PASS",
                  "valueFrom": {
                    "secretKeyRef": {
                      "key": "mariadb-password",
                      "name": "mariadbcreds"
                    }
                  }
                },
                {
                  "name": "DB_USER",
                  "value": "hedgedoc"
                }
              ],
              "envFrom": null,
              "ports": [
                {
                  "name": "main",
                  "containerPort": 3000,
                  "protocol": "TCP"
                }
              ],
              "volumeMounts": [
