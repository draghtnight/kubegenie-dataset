[
  {
    "apiVersion": "v1",
    "kind": "ServiceAccount",
    "metadata": {
      "name": "release-name-hummingbot",
      "labels": {
        "helm.sh/chart": "hummingbot-0.2.0",
        "app.kubernetes.io/name": "hummingbot",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "latest",
        "app.kubernetes.io/managed-by": "Helm"
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "StatefulSet",
    "metadata": {
      "name": "release-name-hummingbot",
      "labels": {
        "helm.sh/chart": "hummingbot-0.2.0",
        "app.kubernetes.io/name": "hummingbot",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "latest",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "replicas": 1,
      "serviceName": "release-name-hummingbot",
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "hummingbot",
          "app.kubernetes.io/instance": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app.kubernetes.io/name": "hummingbot",
            "app.kubernetes.io/instance": "release-name"
          }
        },
        "spec": {
          "serviceAccountName": "release-name-hummingbot",
          "securityContext": {
            "fsGroup": 1000
          },
          "containers": [
            {
              "name": "hummingbot",
              "securityContext": {
                "capabilities": {
                  "drop": [
                    "ALL"
                  ]
                },
                "readOnlyRootFilesystem": true,
                "runAsNonRoot": true,
                "runAsUser": 10719,
                "allowPrivilegeEscalation": false
              },
              "image": "coinalpha/hummingbot:latest",
              "imagePullPolicy": "IfNotPresent",
              "tty": true,
              "stdin": true,
              "volumeMounts": [
                {
                  "mountPath": "/data",
                  "name": "release-name-hummingbot-config",
                  "subPath": "data"
                },
                {
                  "mountPath": "/conf",
                  "name": "release-name-hummingbot-config",
                  "subPath": "conf"
                },
                {
                  "mountPath": "/scripts",
                  "name": "release-name-hummingbot-config",
                  "subPath": "scripts"
                },
                {
                  "mountPath": "/logs",
                  "name": "release-name-hummingbot-logs"
                }
              ],
              "resources": {
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              }
            }
          ]
        }
      },
      "volumeClaimTemplates": [
        {
          "metadata": {
            "name": "release-name-hummingbot-config"
          },
          "spec": {
            "accessModes": [
              "ReadWriteOnce"
            ],
            "resources": {
              "requests": {
                "storage": "1Gi"
              }
            }
          }
        },
        {
          "metadata": {
            "name": "release-name-hummingbot-logs"
          },
          "spec": {
            "accessModes": [
              "ReadWriteOnce"
            ],
            "resources": {
              "requests": {
                "storage": "1Gi"
              }
            }
          }
        }
      ]
    }
  },
  {
    "apiVersion": "v1",
    "kind": "Pod",
    "metadata": {
      "name": "release-name-hummingbot-test-connection",
      "labels": {
        "helm.sh/chart": "hummingbot-0.2.0",
        "app.kubernetes.io/name": "hummingbot",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "latest",
        "app.kubernetes.io/managed-by": "Helm"
      },
      "annotations": {
        "helm.sh/hook": "test"
      }
    },
    "spec": {
      "containers": [
        {
          "name": "wget",
          "image": "busybox",
          "command": [
            "wget"
          ],
          "args": [
            "release-name-hummingbot:7681"
          ]
        }
      ],
      "restartPolicy": "Never"
    }
  }
]