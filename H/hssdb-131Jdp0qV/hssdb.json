[
  {
    "apiVersion": "v1",
    "kind": "Secret",
    "metadata": {
      "name": "hssdb-keys",
      "namespace": "131Jdp0qV"
    },
    "type": "Opaque",
    "data": {
      "id_rsa": ""
    }
  },
  {
    "apiVersion": "v1",
    "kind": "ConfigMap",
    "metadata": {
      "name": "hssdb"
    },
    "data": {
      "serviceConfig": "name: hssdb\naccessor:\n  username: \"admin@opencord.org\"\n  password: \"letmein\"\n  endpoint: xos-core:50051\nlogging:\n  version: 1\n  handlers:\n    console:\n      class: logging.StreamHandler\n    file:\n      class: logging.handlers.RotatingFileHandler\n      filename: /var/log/xos.log\n      maxBytes: 10485760\n      backupCount: 5\n    kafka:\n      class: kafkaloghandler.KafkaLogHandler\n      bootstrap_servers:\n        - \"cord-kafka:9092\"\n      topic: xos.log.hssdb\n  loggers:\n    '':\n      handlers:\n        - console\n        - file\n        - kafka\n      level: DEBUG\nproxy_ssh:\n  enabled: true\n  user: root\n"
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-hssdb",
      "labels": {
        "app": "hssdb",
        "chart": "hssdb-1.0.4",
        "release": "release-name",
        "heritage": "Helm"
      }
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app": "hssdb",
          "release": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app": "hssdb",
            "release": "release-name"
          },
          "annotations": {
            "checksum/config": "572fdad56c88587ae17e9e0a8ed74f4c68f39dfdd4ed4bbdacceeefd7d780804"
          }
        },
        "spec": {
          "containers": [
            {
              "name": "hssdb",
              "image": "xosproject/hssdb-synchronizer:1.0.2",
              "imagePullPolicy": "Always",
              "resources": {
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              },
              "volumeMounts": [
                {
                  "name": "hssdb-config",
                  "mountPath": "/opt/xos/synchronizers/hssdb/mounted_config.yaml",
                  "subPath": "mounted_config.yaml"
                },
                {
                  "name": "certchain-volume",
                  "mountPath": "/usr/local/share/ca-certificates/local_certs.crt",
                  "subPath": "config/ca_cert_chain.pem"
                },
                {
                  "name": "hssdb-keys",
                  "mountPath": "/opt/xos/services/hssdb/keys",
                  "readOnly": true
                },
                {
                  "name": "node-key",
                  "mountPath": "/opt/cord_profile",
                  "readOnly": true
                }
              ]
            }
          ],
          "volumes": [
            {
              "name": "hssdb-config",
              "configMap": {
                "name": "hssdb",
                "items": [
                  {
                    "key": "serviceConfig",
                    "path": "mounted_config.yaml"
                  }
                ]
              }
            },
            {
              "name": "certchain-volume",
              "configMap": {
                "name": "ca-certificates",
                "items": [
                  {
                    "key": "chain",
                    "path": "config/ca_cert_chain.pem"
                  }
                ]
              }
            },
            {
              "name": "hssdb-keys",
              "secret": {
                "secretName": "hssdb-keys",
                "defaultMode": 256
              }
            },
            {
              "name": "node-key",
              "secret": {
                "secretName": "node-key",
                "defaultMode": 256,
                "optional": true
              }
            }
          ]
        }
      }
    }
  }
]