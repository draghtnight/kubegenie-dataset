                  "value": "60"
                },
                {
                  "name": "HASURA_GRAPHQL_ENABLE_MAINTENANCE_MODE",
                  "value": "false"
                },
                {
                  "name": "HASURA_GRAPHQL_CONNECTION_COMPRESSION",
