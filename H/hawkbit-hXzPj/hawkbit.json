                    "secretKeyRef": {
                      "name": "release-name-hawkbit-rabbitmq-pass",
                      "key": "rabbitmq-pass"
                    }
                  }
                },
                {
                  "name": "org.eclipse.hawkbit.repository.file.path",
                  "value": "/var/lib/hawkbit-storage"
                }
              ],
              "ports": [
                {
                  "name": "http",
                  "containerPort": 8080,
                  "protocol": "TCP"
                }
