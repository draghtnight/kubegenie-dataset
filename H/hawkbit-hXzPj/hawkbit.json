              "readinessProbe": {
                "httpGet": {
                  "path": "/VAADIN/themes/hawkbit/favicon.ico",
                  "port": "http"
                },
                "initialDelaySeconds": 120,
                "timeoutSeconds": 5
              },
              "volumeMounts": [
                {
                  "name": "configmap",
                  "mountPath": "/opt/hawkbit/config"
