[
  {
    "apiVersion": "v1",
    "kind": "Secret",
    "metadata": {
      "name": "vspgwu-keys",
      "namespace": "VwadSv69"
    },
    "type": "Opaque",
    "data": {
      "id_rsa": ""
    }
  },
  {
    "apiVersion": "v1",
    "kind": "ConfigMap",
    "metadata": {
      "name": "vspgwu"
    },
    "data": {
      "serviceConfig": "name: vspgwu\naccessor:\n  username: \"admin@opencord.org\"\n  password: \"letmein\"\n  endpoint: xos-core:50051\nlogging:\n  version: 1\n  handlers:\n    console:\n      class: logging.StreamHandler\n    file:\n      class: logging.handlers.RotatingFileHandler\n      filename: /var/log/xos.log\n      maxBytes: 10485760\n      backupCount: 5\n    kafka:\n      class: kafkaloghandler.KafkaLogHandler\n      bootstrap_servers:\n        - \"cord-kafka:9092\"\n      topic: xos.log.vspgwu\n  loggers:\n    '':\n      handlers:\n        - console\n        - file\n        - kafka\n      level: DEBUG\nblueprints:\n  - name: cord_5_0_blueprint\n    graph:\n      - name: VMMETenant\n      - name: VSPGWCTenant\n        links:\n          - name: VMMETenant\n          - name: VSPGWUTenant\n      - name: VSPGWUTenant\n      - name: VHSSTenant\n        links:\n          - name: HSSDBServiceInstance\n      - name: HSSDBServiceInstance\n  - name: cord_4_1_blueprint\n    graph:\n      - name: VSPGWUTenant\n        links:\n          - name: VENBServiceInstance\n      - name: VENBServiceInstance\n      - name: VSPGWCTenant\n        links:\n          - name: VENBServiceInstance\n          - name: VSPGWUTenant\nproxy_ssh:\n  enabled: true\n  user: root\n"
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-vspgwu",
      "labels": {
        "app": "vspgwu",
        "chart": "vspgwu-1.0.4",
        "release": "release-name",
        "heritage": "Helm"
      }
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app": "vspgwu",
          "release": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app": "vspgwu",
            "release": "release-name"
          },
          "annotations": {
            "checksum/config": "e28bab951afa9a97db0379dad5e554e802ae01836074328c80709a7169ddded6"
          }
        },
        "spec": {
          "containers": [
            {
              "name": "vspgwu",
              "image": "xosproject/vspgwu-synchronizer:1.0.2",
              "imagePullPolicy": "Always",
              "resources": {
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              },
              "volumeMounts": [
                {
                  "name": "vspgwu-config",
                  "mountPath": "/opt/xos/synchronizers/vspgwu/mounted_config.yaml",
                  "subPath": "mounted_config.yaml"
                },
                {
                  "name": "certchain-volume",
                  "mountPath": "/usr/local/share/ca-certificates/local_certs.crt",
                  "subPath": "config/ca_cert_chain.pem"
                },
                {
                  "name": "vspgwu-keys",
                  "mountPath": "/opt/xos/services/vspgwu/keys",
                  "readOnly": true
                },
                {
                  "name": "node-key",
                  "mountPath": "/opt/cord_profile",
                  "readOnly": true
                }
              ]
            }
          ],
          "volumes": [
            {
              "name": "vspgwu-config",
              "configMap": {
                "name": "vspgwu",
                "items": [
                  {
                    "key": "serviceConfig",
                    "path": "mounted_config.yaml"
                  }
                ]
              }
            },
            {
              "name": "certchain-volume",
              "configMap": {
                "name": "ca-certificates",
                "items": [
                  {
                    "key": "chain",
                    "path": "config/ca_cert_chain.pem"
                  }
                ]
              }
            },
            {
              "name": "vspgwu-keys",
              "secret": {
                "secretName": "vspgwu-keys"
              }
            },
            {
              "name": "node-key",
              "secret": {
                "secretName": "node-key",
                "defaultMode": 256,
                "optional": true
              }
            }
          ]
        }
      }
    }
  }
]